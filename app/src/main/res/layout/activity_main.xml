<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:colortextview="http://schemas.android.com/apk/res-auto"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:background="@android:color/white"
    android:padding="16dp">


    <loic.teillard.colortextview.ColorTextView
        android:id="@+id/colorTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:textSize="26dp"
        android:textStyle="bold"
        colortextview:clt_color1="#5D4037"
        colortextview:clt_color2="#F4511E"
        colortextview:clt_spaces="true"
        colortextview:clt_txt1="Si"
        colortextview:clt_txt2="Praktis"
        colortextview:layout_constraintBottom_toBottomOf="parent"
        colortextview:layout_constraintEnd_toEndOf="parent"
        colortextview:layout_constraintHorizontal_bias="0.502"
        colortextview:layout_constraintStart_toStartOf="parent"
        colortextview:layout_constraintTop_toTopOf="parent"
        colortextview:layout_constraintVertical_bias="0.11" />

        <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:textStyle="bold"-->
            <!--android:textSize="30dp"-->
            <!--android:text="@string/si_praktis"/>-->


    <TextView
        android:id="@+id/tv_paskrip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_skripsi"
        android:layout_alignEnd="@+id/btn_skripsi"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="8dp"
        android:text="Paskrip"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        colortextview:layout_constraintEnd_toEndOf="parent"
        colortextview:layout_constraintHorizontal_bias="0.28"
        colortextview:layout_constraintStart_toStartOf="parent"
        colortextview:layout_constraintTop_toBottomOf="@+id/btn_skripsi" />


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/btn_skripsi"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="68dp"
        android:layout_marginTop="156dp"
        android:background="@drawable/skripsi"
        android:onClick="skripsi"
        android:paddingTop="20dp"
        android:textColor="#fff"
        colortextview:layout_constraintStart_toStartOf="parent"
        colortextview:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_below="@+id/btn_ilmiah"
        android:layout_alignEnd="@+id/btn_ilmiah"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="8dp"
        android:text="Pakar"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        colortextview:layout_constraintBottom_toBottomOf="parent"
        colortextview:layout_constraintEnd_toEndOf="parent"
        colortextview:layout_constraintHorizontal_bias="0.962"
        colortextview:layout_constraintStart_toStartOf="parent"
        colortextview:layout_constraintTop_toBottomOf="@+id/btn_ilmiah"
        colortextview:layout_constraintVertical_bias="0.008" />


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/btn_ilmiah"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="156dp"
        android:layout_marginEnd="8dp"
        android:layout_toRightOf="@id/btn_skripsi"
        android:background="@drawable/ilimiah"
        android:paddingTop="20dp"
        android:onClick="pakar"
        android:textColor="#fff"
        colortextview:layout_constraintEnd_toEndOf="parent"
        colortextview:layout_constraintHorizontal_bias="0.773"
        colortextview:layout_constraintStart_toStartOf="parent"
        colortextview:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/tv_dikti"
        android:layout_width="96dp"
        android:layout_height="17dp"
        android:layout_below="@id/btn_dikti"
        android:layout_alignParentStart="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="SIMBELMAWA"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        colortextview:layout_constraintBottom_toBottomOf="parent"
        colortextview:layout_constraintEnd_toEndOf="parent"
        colortextview:layout_constraintHorizontal_bias="0.236"
        colortextview:layout_constraintStart_toStartOf="parent"
        colortextview:layout_constraintTop_toBottomOf="@+id/btn_dikti"
        colortextview:layout_constraintVertical_bias="0.0" />


    <ImageButton
        android:id="@+id/btn_dikti"
        android:layout_width="81dp"
        android:layout_height="66dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/dikti"
        android:onClick="simbelmawa"
        android:paddingTop="20dp"
        android:textColor="#fff"
        colortextview:layout_constraintEnd_toEndOf="parent"
        colortextview:layout_constraintHorizontal_bias="0.26"
        colortextview:layout_constraintStart_toStartOf="parent"
        colortextview:layout_constraintTop_toBottomOf="@+id/tv_paskrip" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/btn_about"
        android:layout_width="68dp"
        android:layout_height="65dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/about"
        android:paddingTop="20dp"
        android:textColor="#fff"
        colortextview:layout_constraintEnd_toEndOf="parent"
        colortextview:layout_constraintHorizontal_bias="0.769"
        colortextview:layout_constraintStart_toStartOf="parent"
        colortextview:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/tv_about"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tv_dikti"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:layout_toRightOf="@+id/tv_dikti"
        android:text="@string/about"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        colortextview:layout_constraintBottom_toBottomOf="parent"
        colortextview:layout_constraintEnd_toEndOf="parent"
        colortextview:layout_constraintHorizontal_bias="0.407"
        colortextview:layout_constraintStart_toEndOf="@+id/tv_dikti"
        colortextview:layout_constraintTop_toBottomOf="@+id/btn_about"
        colortextview:layout_constraintVertical_bias="0.0" />


        <!--</RelativeLayout>-->
    <!--</RelativeLayout>-->
</android.support.constraint.ConstraintLayout>